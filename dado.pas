             writeln(save,'numero 4: ',(num[4]/test)*100 :4:3,'%');
             writeln(save,'numero 5: ',(num[5]/test)*100 :4:3,'%');
             writeln(save,'numero 6: ',(num[6]/test)*100 :4:3,'%');
             writeln(save);
             writeln(save,'numero prove: ',test);
             close(save);
             writeln('salvato nel file ',percorso+'.txt');
             writeln;
             writeln('premere ''q'' per uscire,');
             writeln('        ''r'' per lanciare ancora il dado');
             repeat
                 c:=readkey;
             until (c='r')or(c='q');
        end;
     until c='q';
end.
