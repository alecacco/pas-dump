procedure menu;
var k:integer;
    commands:array[1..5]of string;
begin
    textcolor(lightgray);
    textbackground(black);
    clrscr;
    title;
    textcolor(lightgray);
    textbackground(black);
    gotoxy(25,23);
    writeln('copyright Alessandro Cacco 2013');
    loadteams;
    mode:=1;
    myteam:=0;
    textcolor(black);
    commands[1]:='----normal mode---';
    commands[2]:='----crazy mode----';
    commands[3]:='-----fog mode-----';
    commands[4]:='----night mode----';
    commands[5]:='-----quit game----';
    repeat
          k:=749;
          textbackground(lightgray);
          for i:=1 to 5 do
                  begin
                       gotoxy(31,11+(i*2));
                       write(commands[i]);
                  end;
          writeteam;
          repeat
              k:=k+1;
              gotoxy(31,11+(mode*2));
              if k mod 1500=750 then  textbackground(yellow);
              if k mod 1500=0 then  textbackground(lightgray);
              write(commands[mode]);
          until keypressed;
          c:=readkey;
          case c of
               'w': mode:=mode-1;
               's': mode:=mode+1;
               'r':
                   begin
                        mode:=random(4)+1;
                        myteam:=random(9);
                   end;
               #0:
                  begin
                       c:=readkey;
                       case c of
                            #72:mode:=mode-1;
                            #80:mode:=mode+1;
                            #75:myteam:=myteam-1;
                            #77:myteam:=myteam+1;
                       end;
                  end;
          end;
          if myteam=-1 then myteam:=8;
          if myteam=9 then myteam:=0;
          if mode=0 then mode:=5;
          if mode=6 then mode:=1;
          if c=#13 then c:=' ';
    until (c=' ');
    if mode=4 then textbackground(0)
              else textbackground(7);
    clrscr;
end;

