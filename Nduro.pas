procedure writescreen(forced:boolean);
begin
       for i:=0 to enemies do
           with m[i]do
                  if (((y-(m[0].y-44)>0)and(y-(m[0].y-44)<=50))and(mode<>3))or((mode=3)and((y-(m[0].y-44)>20)and(y-(m[0].y-44)<=50)))then
                    if (exx<>x)or(exy-(m[0].exy-44)<>y-(m[0].y-44))or(forced) then
                      if mode<>4 then
                       begin
                          textcolor(col);
                          gotoxy(x,y-(m[0].y-44));
                          write('0-T-0');                                         
                          if (y-(m[0].y-44)+1<=50) then
                             begin
                                  gotoxy(x,y-(m[0].y-44)+1);
                                  write(' |8| ');
                             end;                                                 
                          if (y-(m[0].y-44)+2<=50) then
                             begin
                                  gotoxy(x,y-(m[0].y-44)+2);
                                  write('0<^>0');
                             end;
                       end
                       else
                       begin
                          textbackground(0);
                          textcolor(white);
                          gotoxy(x,y-(m[0].y-44));
                          write('-   -');
                          textcolor(col);
                          if (y-(m[0].y-44)+1<=50) then
                             begin
                                  gotoxy(x,y-(m[0].y-44)+1);
                                  write('  *  ');
                             end;                                                 
                          textcolor(lightred);
                          if (y-(m[0].y-44)+2<=50) then
                             begin
                                  gotoxy(x,y-(m[0].y-44)+2);
                                  write('o   o');
                             end;
                       end;
      if mode=4 then textbackground(black)
               else textbackground(7);
      textcolor(lightred);
      gotoxy(19,51-(m[0].y mod 50)-1);
      write(' ');
      if (51- (m[0].y mod 50)-1>30)or(51-(m[0].y mod 50)-1<9) then
        begin
             gotoxy(61,51- m[0].y mod 50-1);
             write(' ');
        end;
      if m[0].y mod 50>0 then
       begin
         gotoxy(19,51- m[0].y mod 50);
         write('o');
         if (51- (m[0].y mod 50)>30)or(51-( m[0].y mod 50)<9) then
           begin
                gotoxy(61,51- m[0].y mod 50);
                write('o');
           end;
       end;
end;

