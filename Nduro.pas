procedure deleteoldscreen(forced:boolean);
begin
     textcolor(white);
     if mode=4 then textbackground(black)
               else textbackground(white);
       for i:=0 to enemies do
           with m[i]do
                  if (exx<>x)or(exy-(m[0].exy-44)<>y-(m[0].y-44))or(forced) then
                    if (((exy-(m[0].exy-44)>0)and(exy-(m[0].exy-44)<=50))and(mode<>3))or((mode=3)and((exy-(m[0].exy-44)>20)and(exy-(m[0].exy-44)<=50)))then
                     begin
                          gotoxy(exx,exy-(m[0].exy-44));
                          write('     ');
                          if (y-(m[0].y-44)+1<50) then
                             begin
                                  gotoxy(exx,exy-(m[0].exy-44)+1);
                                  write('     ');
                             end;
                          if (y-(m[0].y-44)+2<50) then
                             begin
                                  gotoxy(exx,exy-(m[0].exy-44)+2);
                                  write('     ');
                             end;
                     end;
end;

