function collision(ics,ipsilon:integer):boolean;
begin
     collision:=false;
     for j:=0 to enemies do
         with m[j] do
              if (j<>i)and(y+3>=ipsilon)and(y-3<ipsilon)and(ics>x-5)and(ics<x+5) then collision:=true;
     if collision then writescreen(true);
end;

procedure crashslowdown;
begin
     for i:=0 to enemies do
         with m[i] do
              if collision(x,y)=true then v:=15;
     i:=0;
     if collision(m[0].x,m[0].y) then
        begin
             points:=points-2;
             gear:=1;
             writegear;
        end;
end;

procedure increasehyp;
begin
     if (t mod 50=0)and(hyp<10) then
        begin
             hyp:=hyp+1;
             writehyp;
        end;
end;

procedure movimento;
begin
     for i:=0 to enemies do
         if (t mod m[i].v=0)or(t+1 mod m[i].v=0) then m[i].y := (m[i].y-1);
end;

procedure verifica;
begin
     with m[0]do
       begin
          case c of
               'a':x:=x-steer;
               'd':x:=x+steer;
               's':
                   begin
                        if v<5 then v:=v+1
                                else v:=v+5;
                        gear:=gear-1;
                   end;
               'w':
                   begin
                        if v<10 then v:=v-1
                                else v:=v-5;
                        gear:=gear+1;
                   end;
               'j':x:=x-steer;
               'l':x:=x+steer;
               'k':
                   begin
                        if v<5 then v:=v+1
